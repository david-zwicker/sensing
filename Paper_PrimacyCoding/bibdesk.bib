@article{Avidor-Reiss2012,
	Address = {Department of Biological Sciences, University of Toledo, Toledo, OH 43606, USA. Electronic address: Tomer.AvidorReiss{\char64}utoledo.edu.},
	Author = {Avidor-Reiss, Tomer and Gopalakrishnan, Jayachandran},
	Booktitle = {Current Opinion In Cell Biology},
	Date = {2012/00/27},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1016/j.ceb.2012.10.016},
	Id = {780},
	Journal = {Curr. Opin. Cell Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2012/Avidor-Reiss/Curr.%20Opin.%20Cell%20Biol.%202012%20Avidor-Reiss%20-%20Building%20a%20centriole.pdf},
	Month = {00},
	N2 = {Centrioles are the key foundation of centrosomes and cilia, yet a molecular understanding of how they form has only recently begun to emerge. Building a fully functional centriole that can form a centrosome and cilium requires two cell cycles. Centriole building starts with procentriole nucleation, a process that is coordinated by the conserved proteins Plk4/Zyg-1, and Asterless/Cep152. Subsequently, Sas-6, a conserved procentriole protein, self-assembles to provide nine-fold symmetry to the centriole scaffold. The procentriole then continues to elongate into a centriole, a process controlled by Sas-4/CPAP and CP110. Then, centrioles recruit Sas-4-mediated pre-assembled centrosomal complexes from the cytoplasm to form the pericentriolar material (PCM). Finally, CP110 and its interacting proteins are involved in controlling the timing of centriole templating of the cilium.},
	Number = {1},
	Pages = {72--77},
	Title = {Building a centriole},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0955067412001809},
	Volume = {25},
	Year = {2012},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0955067412001809},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.ceb.2012.10.016}}

@article{Yamashita2009,
	Address = {Life Sciences Institute, Center for Stem Cell Biology, Department of Cell and Developmental Biology, University of Michigan, Ann Arbor, MI 48109-2216, USA. yukikomy{\char64}umich.edu},
	Author = {Yamashita, Yukiko M},
