@article{Doxsey2005,
	Address = {Program in Molecular Medicine, University of Massachusetts Medical School, Worcester, Massachusetts 01605, USA. stephen.doxsey{\char64}umassmed.edu},
	Author = {Doxsey, Stephen J and Zimmerman, Wendy C and Mikule, Keith},
	Booktitle = {Trends In Cell Biology},
	Date = {2005/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
