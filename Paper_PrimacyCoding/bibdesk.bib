@article{Sokolova2013,
	Address = {Institute for Molecules and Materials, Radboud University Nijmegen, 6525 AJ, Nijmegen, The Netherlands.},
	Author = {Sokolova, Ekaterina and Spruijt, Evan and Hansen, Maike M K and Dubuc, Emilien and Groen, Joost and Chokkalingam, Venkatachalam and Piruska, Aigars and Heus, Hans A and Huck, Wilhelm T S},
	Booktitle = {Proceedings Of The National Academy Of Sciences Of The United States Of America},
	Date = {2013/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2015-10-15 21:17:23 +0000},
	Doi = {10.1073/pnas.1222321110},
	Id = {1530},
	Journal = {Proc. Natl. Acad. Sci. USA},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2013/Sokolova/Proc.%20Natl.%20Acad.%20Sci.%20USA%202013%20Sokolova%20-%20Enhanced%20transcription%20rates%20in%20membrane-free%20protocells%20formed%20by%20coacervation%20of%20cell%20lysate.pdf},
	Month = {00},
	N2 = {Liquid-liquid phase transitions in complex mixtures of proteins and other molecules produce crowded compartments supporting in vitro transcription and translation. We developed a method based on picoliter water-in-oil droplets to induce coacervation in Escherichia coli cell lysate and follow gene expression under crowded and noncrowded conditions. Coacervation creates an artificial cell-like environment in which the rate of mRNA production is increased significantly. Fits to the measured transcription rates show a two orders of magnitude larger binding constant between DNA and T7 RNA polymerase, and five to six times larger rate constant for transcription in crowded environments, strikingly similar to in vivo rates. The effect of crowding on interactions and kinetics of the fundamental machinery of gene expression has a direct impact on our understanding of biochemical networks in vivo. Moreover, our results show the intrinsic potential of cellular components to facilitate macromolecular organization into membrane-free compartments by phase separation.},
	Number = {29},
	Pages = {11692},
	Title = {Enhanced transcription rates in membrane-free protocells formed by coacervation of cell lysate.},
	Ty = {JOUR},
	Url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23818642&retmode=ref&cmd=prlinks},
