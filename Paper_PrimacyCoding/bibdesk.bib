@article{Chan2010,
	Address = {Department of Biochemistry and Biophysics; University of California, San Francisco; San Francisco, CA USA.},
	Author = {Chan, Yee-Hung M and Marshall, Wallace F},
	Booktitle = {Organogenesis},
	Date = {2010/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Id = {499},
	Journal = {Organogenesis},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2010/Chan/Organogenesis%202010%20Chan%20-%20Scaling%20properties%20of%20cell%20and%20organelle%20size.pdf},
	Month = {00},
	N2 = {How size is controlled is a fundamental question in biology. In this review, we discuss the use of scaling relationships-for example, power-laws of the form y∝x(α)-to provide a framework for comparison and interpretation of size measurements. Such analysis can illustrate the biological and physical principles underlying observed trends, as has been proposed for the allometric dependence of metabolic rate or limb structure on organism mass. Techniques for measuring size at smaller length-scales continue to improve, leading to more data on the control of size in cells and organelles. Size scaling of these structures is expected to influence growth patterns, functional capacity and intracellular transport. Furthermore, organelles such as the nucleus, mitochondria and endoplasmic reticulum show widely varying morphologies that affect their scaling properties. We provide brief summaries of these issues for individual organelles, and conclude with a discussion on how to apply this concept to better understand the mechanisms of size control in the cellular environment.},
	Number = {2},
	Pages = {88--96},
	Title = {Scaling properties of cell and organelle size},
	Ty = {JOUR},
	Url = {http://www.landesbioscience.com/journals/organogenesis/article/11464/?nocache=1510253584},
	Volume = {6},
	Year = {2010},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.landesbioscience.com/journals/organogenesis/article/11464/?nocache=1510253584}}

@article{Marshall2009,
	Address = {Dept. of Biochemistry and Biophysics, University of California San Francisco, San Francisco, CA 94158, USA. wmarshall{\char64}biochem.ucsf.edu},
	Author = {Marshall, Wallace F},
	Booktitle = {Current Opinion In Cell Biology},
	Date = {2009/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1016/j.ceb.2009.01.008},
	Id = {500},
	Journal = {Curr. Opin. Cell Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2009/Marshall/Curr.%20Opin.%20Cell%20Biol.%202009%20Marshall%20-%20Centriole%20evolution.pdf},
	Month = {00},
	N2 = {Centrioles are cylindrical structures found at the core of the mitotic spindle pole, which also act as basal bodies to nucleate the formation of cilia. Centrioles have a complex, ninefold symmetric structure, and reproduce by an intriguing duplication process. The complexity and apparent self-reproduction of centrioles raises the question of how such a structure could have evolved, making them a favorite topic for theological speculation by 'intelligent design' creationists. In fact, centrioles are capable of robust self-assembly and can tolerate dramatic perturbations while still maintaining basic functionality. Far from being irreducibly complex, centrioles appear to be based on a rather minimal underlying core structure requiring only a handful of genes to construct.},
	Number = {1},
	Pages = {14--19},
	Title = {Centriole evolution},
	Ty = {JOUR},
