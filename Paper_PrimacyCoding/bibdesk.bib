@article{Schlaitz2007,
	Address = {MPI CBG, D-01307 Dresden, Germany},
	Author = {Schlaitz, Anne-Lore and Srayko, Martin and Dammermann, Alexander and Quintin, Sohie and Wielsch, Natalie and MacLeod, Ian and de Robillard, Quentin and Zinke, Andrea and Yates, John R and M{\"u}ller-Reichert, Thomas and Shevchenko, Andrei and Oegema, Karen and Hyman, Anthony A},
	Booktitle = {Cell},
