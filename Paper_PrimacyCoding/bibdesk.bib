@article{Audhya2005,
	Address = {Ludwig Institute for Cancer Research, Department of Cellular and Molecular Medicine, University of California, San Diego, La Jolla, CA 92093, USA. aaudhya{\char64}ucsd.edu},
	Author = {Audhya, Anjon and Hyndman, Francie and McLeod, Ian X and Maddox, Amy S and Yates, John R and Desai, Arshad and Oegema, Karen},
	Booktitle = {The Journal of Cell Biology},
	Date = {2005/00/24},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1083/jcb.200506124},
	Id = {808},
	Journal = {J. Cell Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2005/Audhya/J.%20Cell%20Biol.%202005%20Audhya%20-%20A%20complex%20containing%20the%20Sm%20protein%20CAR-1%20and%20the%20RNA%20helicase%20CGH-1%20is%20required%20for%20embryonic%20cytokinesis%20in%20Caenorhabditis%20elegans.pdf},
	Month = {00},
	N2 = {Cytokinesis completes cell division and partitions the contents of one cell to the two daughter cells. Here we characterize CAR-1, a predicted RNA binding protein that is implicated in cytokinesis. CAR-1 localizes to germline-specific RNA-containing particles and copurifies with the essential RNA helicase, CGH-1, in an RNA-dependent fashion. The atypical Sm domain of CAR-1, which directly binds RNA, is dispensable for CAR-1 localization, but is critical for its function. Inhibition of CAR-1 by RNA-mediated depletion or mutation results in a specific defect in embryonic cytokinesis. This cytokinesis failure likely results from an anaphase spindle defect in which interzonal microtubule bundles that recruit Aurora B kinase and the kinesin, ZEN-4, fail to form between the separating chromosomes. Depletion of CGH-1 results in sterility, but partially depleted worms produce embryos that exhibit the CAR-1-depletion phenotype. Cumulatively, our results suggest that CAR-1 functions with CGH-1 to regulate a specific set of maternally loaded RNAs that is required for anaphase spindle structure and cytokinesis.},
	Number = {2},
	Pages = {267--279},
	Title = {A complex containing the Sm protein CAR-1 and the RNA helicase CGH-1 is required for embryonic cytokinesis in Caenorhabditis elegans},
	Ty = {JOUR},
	Url = {http://jcb.rupress.org/content/171/2/267.long},
	Volume = {171},
	Year = {2005},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://jcb.rupress.org/content/171/2/267.long},
	Bdsk-Url-2 = {http://dx.doi.org/10.1083/jcb.200506124}}

