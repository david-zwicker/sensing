	Mesh = {Agar; Amino Acids; Culture Media; Ecosystem; Evolution, Molecular; Genetic Drift; Genetic Variation; Microscopy, Fluorescence; Models, Theoretical; Mutation; Saccharomyces cerevisiae; Symbiosis},
