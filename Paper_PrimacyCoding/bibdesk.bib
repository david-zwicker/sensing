@article{Cowan2004,
	Address = {Max Planck Inst Mol Cell Biol \& Genet, D-01307 Dresden, Germany},
	Author = {Cowan, Carrie R and Hyman, Anthony A},
	Booktitle = {Annual Review of Cell And Developmental Biology},
	Date = {2004/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1146/annurev.cellbio.19.111301.113823},
	Id = {284},
	Journal = {Annu. Rev. Cell Dev. Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2004/Cowan/Annu.%20Rev.%20Cell%20Dev.%20Biol.%202004%20Cowan%20-%20Asymmetric%20cell%20division%20in%20C-elegans%20Cortical%20polarity%20and%20spindle%20positioning.pdf},
	Month = {00},
	N2 = {The one-cell Caenorhabditis elegans embryo divides asymmetrically into a larger and smaller blastomere, each with a different fate. How does such asymmetry arise? The sperm-supplied centrosome establishes an axis of polarity in the embryo that is transduced into the establishment of anterior and posterior cortical domains. These cortical domains define the polarity of the embryo, acting upstream of the PAR proteins. The PAR proteins, in turn, determine the subsequent segregation of fate determinants and the plane of cell division. We address how cortical asymmetry could be established, relying on data from C. elegans and other polarized cells, as well as from applicable models. We discuss how cortical polarity influences spindle position to accomplish an asymmetric division, presenting the current models of spindle orientation and anaphase spindle displacement. We focus on asymmetric cell division as a function of the actin and microtubule cytoskeletons, emphasizing the cell biology of polarity.},
	Pages = {427--453},
	Title = {Asymmetric cell division in C-elegans: Cortical polarity and spindle positioning},
	Ty = {JOUR},
	Volume = {20},
	Year = {2004},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1146/annurev.cellbio.19.111301.113823}}

@article{Gadde2004,
	Address = {Univ Calif Berkeley, Dept Mol \& Cell Biol, Berkeley, CA 94720 USA},
	Author = {Gadde, S and Heald, R},
	Booktitle = {Current Biology},
	Date = {2004/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1016/j.cub.2004.09.021},
	Id = {285},
	Journal = {Curr. Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2004/Gadde/Curr.%20Biol.%202004%20Gadde%20-%20Mechanisms%20and%20molecules%20of%20the%20mitotic%20spindle.pdf},
	Month = {00},
	N2 = {In all eukaryotes, morphogenesis of the microtubule cytoskeleton into a bipolar spindle is required for the faithful transmission of the genome to the two daughter cells during division. This process is facilitated by the intrinsic polarity and dynamic properties of microtubules and involves many proteins that modulate microtubule organization and stability. Recent work has begun to uncover the molecular mechanisms behind these dynamic events. Here we describe current models and discuss some of the complex repertoire of factors required for spindle assembly and chromosome segregation.},
	Number = {18},
