@article{Bicout1997,
	Address = {NIDDKD, Phys Chem Lab, NIH, Bethesda, MD 20892 USA},
	Author = {Bicout, DJ},
	Booktitle = {Physical Review E},
	Date = {1997/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Id = {267},
	Journal = {Phys. Rev. E},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/1997/Bicout/Phys.%20Rev.%20E%201997%20Bicout%20-%20Green's%20functions%20and%20first%20passage%20time%20distributions%20for%20dynamic%20instability%20of%20microtubules.pdf},
	Month = {00},
	N2 = {It is shown that the dynamic instability process describing the self-assembly and/or disassembly of microtubules is a continuous version of a variant of persistent random walks described by the generalized telegrapher's equation. That is to say, a microtubule is likely to undergo stochastic traveling waves in which catastrophe and rescue events cannot propagate faster than upsilon(-) and upsilon(+), respectively. For this stochastic process, analytic expressions for Green's functions of position and velocity of a microtubule and exact solutions for the first passage time distributions of a microtubule to the nucleating site are obtained. It is shown that, in the omega-->infinity limit, where omega(-1) is the persistence time, the dynamic instability process can be described by a diffusion process in the presence of a drift term that, in fact, is the steady-stare velocity of the microtubule. As a result, the catastrophe time distribution (i.e., the distribution of microtubule lifetimes to the nucleating site) exhibits a power law with an exponential cutoff as F(t{$\backslash$}x(0))similar to t(-3/2)e(-t/tau c), where tau(c) is the time scale at which the drift term and the diffusive term are comparable.},
	Number = {6},
	Pages = {6656--6667},
	Title = {Green's functions and first passage time distributions for dynamic instability of microtubules},
	Ty = {JOUR},
	Volume = {56},
	Year = {1997},
	Bdsk-File-1 = {}}

@article{Kunda2009,
	Address = {UCL, MRC, Mol Cell Biol Lab, London WC1E 6BT, England},
	Author = {Kunda, Patricia and Baum, Buzz},
	Booktitle = {Trends In Cell Biology},
	Date = {2009/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1016/j.tcb.2009.01.006},
	Id = {268},
	Journal = {Trends Cell Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2009/Kunda/Trends%20Cell%20Biol.%202009%20Kunda%20-%20The%20actin%20cytoskeleton%20in%20spindle%20assembly%20and%20positioning.pdf},
	Month = {00},
	N2 = {The most dramatic changes in eukaryotic cytoskeletal organization and dynamics occur during passage through mitosis. Although both spindle self-organization and actin-dependent cytokinesis have long been the subject of intense investigation, it has only recently become apparent that the actin cortex also has a key role during early mitosis. This is most striking in animal cells, in which changes in the actin cytoskeleton drive mitotic cell rounding and cortical stiffening. This mitotic cortex then functions as a foundation for spindle assembly and to guide spindle orientation with respect to extracellular chemical and mechanical cues. Here, we discuss this recent work and the possible role of crosstalk between the mitotic actin cortex and the plus ends of astral microtubules in this process.},
	Number = {4},
	Pages = {174--179},
	Title = {The actin cytoskeleton in spindle assembly and positioning},
	Ty = {JOUR},
	Volume = {19},
	Year = {2009},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.tcb.2009.01.006}}

