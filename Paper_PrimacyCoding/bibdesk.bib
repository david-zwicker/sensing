@article{Hinchcliffe2011,
	Address = {Section of Cellular Dynamics; The Hormel Institute; University of Minnesota; Austin, MN USA.},
	Author = {Hinchcliffe, Edward H},
	Booktitle = {Cell Cycle},
	Date = {2011/00/15},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2015-11-20 14:44:47 +0000},
	Id = {514},
	Journal = {Cell Cycle},
	Keywords = {Centrosome},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2011/Hinchcliffe/Cell%20Cycle%202011%20Hinchcliffe%20-%20The%20centrosome%20and%20bipolar%20spindle%20assembly%20Does%20one%20have%20anything%20to%20do%20with%20the%20other.pdf},
	Month = {00},
	N2 = {In vertebrate somatic cells the centrosome functions as the major microtubule-organizing center (MTOC), which splits and separates to form the poles of the mitotic spindle. However, the role of the centriole-containing centrosome in the formation of bipolar mitotic spindles continues to be controversial. Cells normally containing centrosomes are still able to build bipolar spindles after their centrioles have been removed or ablated. In naturally occurring cellular systems that lack centrioles - such as plant cells and many oocytes - bipolar spindles form in the complete absence of canonical centrosomes. These observations have led to the notion that centrosomes play no role during mitosis. However, recent work has re-examined spindle assembly in the absence of centrosomes, both in cells that naturally lack them, and those that have had them experimentally removed. The results of these studies suggest that an appreciation of microtubule network organization- both before and after nuclear envelope breakdown (NEB) - is the key to understanding the mechanisms that regulate spindle assembly and the generation of bipolarity.},
	Number = {22 SP -},
	Title = {The centrosome and bipolar spindle assembly: Does one have anything to do with the other?},
	Ty = {JOUR},
	Url = {http://www.landesbioscience.com/journals/cc/article/18293/},
	Volume = {10},
	Year = {2011},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.landesbioscience.com/journals/cc/article/18293/}}

