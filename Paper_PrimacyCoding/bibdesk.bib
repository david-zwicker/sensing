@article{Sillibourne2011,
	Address = {Institut Curie, UMR144, 26 rue d'Ulm, 75248, Paris Cedex 05, France.},
	Author = {Sillibourne, James E and Specht, Christian G and Izeddin, Ignacio and Hurbain, Ilse and Tran, Phong and Triller, Antoine and Darzacq, Xavier and Dahan, Maxime and Bornens, Michel},
	Booktitle = {Cytoskeleton (Hoboken, NJ)},
	Date = {2011/00/04},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1002/cm.20536},
	Id = {502},
	Journal = {Cytoskeleton (Hoboken, NJ) SP -},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2011/Sillibourne/Cytoskeleton%20(Hoboken%20NJ)%202011%20Sillibourne%20-%20Assessing%20the%20localization%20of%20centrosomal%20proteins%20by%20PALMSTORM%20nanoscopy.pdf},
	Month = {00},
	N2 = {The structure of the centrosome was resolved by EM many years ago to reveal a pair of centrioles embedded in a dense network of proteins. More recently, the molecular composition of the centrosome was catalogued by mass spectroscopy and many novel components were identified. Determining precisely where a novel component localizes to within the centrosome remains a challenge, and until now it has required the use of immuno-EM. This technique is both time-consuming and unreliable, as it often fails due to problems with antigen accessibility. We have investigated the use of two nanoscopic techniques, photoactivated localization microscopy (PALM) and stochastic optical reconstruction microscopy (STORM), as alternative techniques for localizing centrosomal proteins. The localization of a known centrosomal component, the distal appendage protein Cep164 was investigated by direct STORM (dSTORM) and resolved with a high spatial resolution. We further validated the use of nanoscopic PALM imaging by showing that the previously uncharacterized centrosomal protein CCDC123 (Cep123) localizes to the distal appendages, forming ring-like structures with a diameter of 500 nm. Our results demonstrate that both PALM and STORM imaging have great potential as alternatives to immuno-EM. {\copyright}2011 Wiley Periodicals, Inc.},
	Title = {Assessing the localization of centrosomal proteins by PALM/STORM nanoscopy},
	Ty = {JOUR},
	Url = {http://onlinelibrary.wiley.com/doi/10.1002/cm.20536/abstract;jsessionid=A5FA9441FA6E5EB970EE27D073E1FBAE.d03t04},
	Year = {2011},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://onlinelibrary.wiley.com/doi/10.1002/cm.20536/abstract;jsessionid=A5FA9441FA6E5EB970EE27D073E1FBAE.d03t04},
	Bdsk-Url-2 = {http://dx.doi.org/10.1002/cm.20536}}

@article{Wu2011,
	Address = {*Department of Chemical Engineering, University of Florida, Gainesville FL 32611.},
	Author = {Wu, Jun and Misra, Gaurav and Russell, Robert J and Ladd, Anthony J C and Lele, Tanmay P and Dickinson, Richard B},
	Booktitle = {Molecular Biology of the Cell},
	Date = {2011/00/19},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2015-11-20 14:44:47 +0000},
	Doi = {10.1091/mbc.E11-07-0611},
	Id = {503},
	Journal = {Mol. Biol. Cell SP -},
	Keywords = {Centrosome},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2011/Wu/Mol.%20Biol.%20Cell%202011%20Wu%20-%20Effects%20of%20Dynein%20on%20Microtubule%20Mechanics%20and%20Centrosome%20Positioning.pdf},
	Month = {00},
	N2 = {In order to determine forces on intracellular microtubules, we measured shape changes of individual microtubules following laser severing. Surprisingly, regions near newly created minus ends increased in curvature following severing, while regions near new microtubule plus ends depolymerized without any observable change in shape. With dynein inhibited, regions near severed minus ends straightened rapidly following severing. These observations suggest that dynein exerts a pulling force on the microtubule which buckles the newly created minus end. Moreover, the lack of any observable straightening suggests that dynein prevents lateral motion of microtubules. To explain these results, we developed a model for intracellular microtubule mechanics which predicts the enhanced buckling at the minus end of a severed microtubule. Our results show that microtubule shapes reflect a dynamic force balance, in which dynein motor and friction forces dominate elastic forces arising from bending moments. A centrosomal array of microtubules subjected to dynein pulling forces and resisted by dynein friction is predicted to center on the experimentally observed timescale, with or without the pushing forces derived from microtubule buckling at the cell periphery.},
	Title = {Effects of Dynein on Microtubule Mechanics and Centrosome Positioning},
	Ty = {JOUR},
	Url = {http://www.molbiolcell.org/content/22/24/4834.long},
	Year = {2011},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.molbiolcell.org/content/22/24/4834.long},
	Bdsk-Url-2 = {http://dx.doi.org/10.1091/mbc.E11-07-0611}}

