@article{Thery2007,
	Address = {Inst Curie, CNRS, UMR 144, F-75248 Paris, France},
	Author = {Thery, Manuel and Jimenez-Dalmaroni, Andrea and Racine, Victor and Bornens, Michel and J{\"u}licher, Frank},
	Booktitle = {Nature},
	Date = {2007/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
