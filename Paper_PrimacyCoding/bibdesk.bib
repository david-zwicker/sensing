	Year = {2001},
	Bdsk-File-1 = {}}

@article{McNally2006,
	Address = {Univ Calif Davis, Sect Mol \& Cellular Biol, Davis, CA 95616 USA},
	Author = {McNally, Karen and Audhya, Anjon and Oegema, Karen and McNally, Francis J},
	Booktitle = {The Journal of Cell Biology},
	Date = {2006/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Id = {229},
	Journal = {J. Cell Biol.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2006/McNally/J.%20Cell%20Biol.%202006%20McNally%20-%20Katanin%20controls%20mitotic%20and%20meiotic%20spindle%20length.pdf},
	Month = {00},
	N2 = {Accurate control of spindle length is a conserved feature of eukaryotic cell division. Lengthening of mitotic spindles contributes to chromosome segregation and cytokinesis during mitosis in animals and fungi. In contrast, spindle shortening may contribute to conservation of egg cytoplasm during female meiosis. Katanin is a microtubule-severing enzyme that is concentrated at mitotic and meiotic spindle poles in animals. We show that inhibition of katanin slows the rate of spindle shortening in nocodazole-treated mammalian fibroblasts and in untreated Caenorhabditis elegans meiotic embryos. Wildtype C. elegans meiotic spindle shortening proceeds through an early katanin-independent phase marked by increasing microtubule density and a second, katanin-dependent phase that occurs after microtubule density stops increasing. In addition, double-mutant analysis indicated that gamma-tubulin-dependent nucleation and microtubule severing may provide redundant mechanisms for increasing microtubule number during the early stages of meiotic spindle assembly.},
	Number = {6},
	Pages = {881--891},
	Title = {Katanin controls mitotic and meiotic spindle length},
	Ty = {JOUR},
	Volume = {175},
	Year = {2006},
	Bdsk-File-1 = {}}

@article{Bouck2008,
	Address = {Univ N Carolina, Dept Biol, Chapel Hill, NC 27599 USA},
	Author = {Bouck, David C and Joglekar, Ajit P and Bloom, Kerry S},
	Booktitle = {Annual Review of Genetics},
	Date = {2008/00/01},
	Date-Added = {2014-07-24 13:27:27 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1146/annurev.genet.42.110807.091620},
	Id = {230},
	Journal = {Annu. Rev. Genet.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2008/Bouck/Annu.%20Rev.%20Genet.%202008%20Bouck%20-%20Design%20Features%20of%20a%20Mitotic%20Spindle%20Balancing%20Tension%20and%20Compression%20at%20a%20Single%20Microtubule%20Kinetochore%20Interface%20in%20Budding%20Yeast.pdf},
	Month = {00},
	N2 = {Accurate segregation of duplicated chromosomes ensures that daughter cells get one and only one copy of each chromosome. Errors ill chromosome segregation result ill aneuploidy and have severe consequences on human health. Incorrect chromosome number and chromosomal instability are hallmarks of tumor cells. Hence, segregation errors are thought to be a Major Cause of tumorigenesis. A study of the physical mechanical basis of chromosome segregation is essential to understand the processes that call lead to errors. Tremendous progress has been made in recent years in identifying the proteins necessary for chromosome movement and segregation, but the mechanism and structure of critical force generating components and the molecular basis of centrosome stiffness remain poorly understood.},
	Pages = {335--359},
