@article{Bourouina2012,
	Address = {Institut Curie, Centre de Recherche, CNRS, UMR168-Paris, F-75248 France.},
	Author = {Bourouina, Nadia and Husson, Julien and Hivroz, Claire and Henry, Nelly},
	Booktitle = {Langmuir},
	Date = {2012/00/10},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1021/la300398a},
	Id = {579},
	Journal = {Langmuir},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2012/Bourouina/Langmuir%202012%20Bourouina%20-%20Biomimetic%20droplets%20for%20artificial%20engagement%20of%20living%20cell%20surface%20receptors%20the%20specific%20case%20of%20the%20T-cell.pdf},
	Month = {00},
	N2 = {Liquid colloids, in the form of droplets grafted with specific biomolecules, are emerging as potential biomimetic systems. Here we show for the first time the possibility of forming hybrid conjugates between an advanced living cell model, the T-cell of the Jurkat cell line, and a specifically grafted droplet. Using T-cells expressing a fluorescent chimeric protein associated with the TCR/CD3 complex and fluorescent ligand-grafted droplets, we demonstrate formation of an interfacial contact concentrated in linking molecules, the morphology and dynamics of which strongly depend on the targeted receptor. The sequence of events ranges from the initial concentration of molecules following an unbound molecule gradient to active actin-driven spreading and fragmentation of the contact, ending with droplet internalization. We observed synchronized colocalization of receptors and ligands driven by cell dynamics and closely mirrored by the droplet interface. Using intracellular calcium probe Fura-2, we also showed that the cell/droplet interaction can trigger the T-cell signaling cascade. By examining molecular dynamics using FRAP measurements, we observed a nearly frozen cell droplet joining interface. Taken together, our results point to liquid colloids as promising new tools both for probing cell surface interactions and receptor dynamics and for manipulating biological cell functions.},
	Number = {14},
	Pages = {6106--6113},
	Title = {Biomimetic droplets for artificial engagement of living cell surface receptors: the specific case of the T-cell},
	Ty = {JOUR},
	Url = {http://pubs.acs.org/doi/abs/10.1021/la300398a},
	Volume = {28},
	Year = {2012},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://pubs.acs.org/doi/abs/10.1021/la300398a},
	Bdsk-Url-2 = {http://dx.doi.org/10.1021/la300398a}}

