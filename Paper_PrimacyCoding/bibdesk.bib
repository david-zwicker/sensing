	Author = {Stenoien, D L and Sen, S and Mancini, M A and Brinkley, B R},
	Booktitle = {Cell Motility And The Cytoskeleton},
	Date = {2003/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
