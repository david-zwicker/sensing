@article{Wang2012,
	Address = {Molecular Design Institute and Department of Chemistry, New York University, New York, New York 10003, USA.},
	Author = {Wang, Yufeng and Wang, Yu and Breed, Dana R and Manoharan, Vinothan N and Feng, Lang and Hollingsworth, Andrew D and Weck, Marcus and Pine, David J},
	Booktitle = {Nature},
	Date = {2012/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
