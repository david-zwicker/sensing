@article{Han2012,
	Address = {Department of Biochemistry, UT Southwestern Medical Center, 5323 Harry Hines Boulevard, Dallas, TX 75390-9152, USA.},
	Author = {Han, Tina W and Kato, Masato and Xie, Shanhai and Wu, Leeju C and Mirzaei, Hamid and Pei, Jimin and Chen, Min and Xie, Yang and Allen, Jeffrey and Xiao, Guanghua and McKnight, Steven L},
