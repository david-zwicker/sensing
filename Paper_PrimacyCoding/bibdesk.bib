@article{Jacobs2013,
	Address = {Department of Chemistry, University of Cambridge, Lensfield Road, Cambridge CB2 1EW, United Kingdom.},
	Author = {Jacobs, William M and Frenkel, Daan},
	Booktitle = {The Journal of Chemical Physics},
	Date = {2013/00/14},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1063/1.4812461},
	Id = {1696},
	Journal = {J. Chem. Phys.},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2013/Jacobs/J.%20Chem.%20Phys.%202013%20Jacobs%20-%20Predicting%20phase%20behavior%20in%20multicomponent%20mixtures.pdf},
	Month = {00},
	N2 = {Mixtures with a large number of components can undergo phase transitions of a hybrid character, with both condensation and demixing contributions. We describe a robust Monte Carlo simulation method for calculating phase coexistence in multicomponent mixtures. We use this approach to study the phase behavior of lattice models of multicomponent mixtures with strongly varying pair interactions. Such a system can be thought of as a simplified model of the cytosol, with both specific and nonspecific interactions. We show that mapping a multicomponent mixture onto an approximately equivalent one-component system yields both upper and lower bounds on the maximum solute volume fraction of a stable, homogeneous phase. By following the minimum excess-free-energy path from the dilute phase free-energy minimum, we predict the difference in composition between the condensed and dilute phases at the boundary of the homogeneous phase. We find that this "direction" of phase separation rarely aligns with the dominant direction of density fluctuations in the dilute phase. We also show that demixing transitions tend to lower the maximum solute volume fraction at which the homogeneous phase is stable. By considering statistical ensembles of mixtures with random interactions, we show that the demixing contribution to phase separation is self-averaging and dependent only on the mean and variance of the distribution of interactions.},
	Number = {2},
	Pages = {024108},
	Title = {Predicting phase behavior in multicomponent mixtures.},
	Ty = {JOUR},
	Url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23862930&retmode=ref&cmd=prlinks},
	Volume = {139},
	Year = {2013},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23862930&retmode=ref&cmd=prlinks},
	Bdsk-Url-2 = {http://dx.doi.org/10.1063/1.4812461}}

