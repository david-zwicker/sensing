	Address = {Dept. Physiology, McGill University, Montr{\'e}al, Qu{\'e}bec, Canada ; Centre for Applied Mathematics in Bioscience and Medicine, Montr{\'e}al, Qu{\'e}bec, Canada ; Meakins-Christie Laboratories, McGill University, Montr{\'e}al, Qu{\'e}bec, Canada.},
	Author = {Hilbert, Lennart and Bates, Genevieve and Roman, Horia N and Blumenthal, Jenna L and Zitouni, Nedjma B and Sobieszek, Apolinary and Mackey, Michael C and Lauzon, Anne-Marie},
