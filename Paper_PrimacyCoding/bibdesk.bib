@article{Kim2013,
	Address = {Laboratory of Metabolism, Center for Cancer Research, National Cancer Institute, Bethesda, MD 20892.},
	Author = {Kim, Tae-Sung and Park, Jung-Eun and Shukla, Anil and Choi, Sunho and Murugan, Ravichandran N and Lee, Jin H and Ahn, Mija and Rhee, Kunsoo and Bang, Jeong K and Kim, Bo Y and Loncarek, Jadranka and Erikson, Raymond L and Lee, Kyung S},
	Booktitle = {Proceedings Of The National Academy Of Sciences Of The United States Of America},
	Date = {2013/00/25},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2014-07-24 13:38:09 +0000},
	Doi = {10.1073/pnas.1319656110},
	Id = {3875},
	Journal = {Proc. Natl. Acad. Sci. USA},
	Month = {00},
	N2 = {Centrosomes play an important role in various cellular processes, including spindle formation and chromosome segregation. They are composed of two orthogonally arranged centrioles, whose duplication occurs only once per cell cycle. Accurate control of centriole numbers is essential for the maintenance of genomic integrity. Although it is well appreciated that polo-like kinase 4 (Plk4) plays a central role in centriole biogenesis, how it is recruited to centrosomes and whether this step is necessary for centriole biogenesis remain largely elusive. Here we showed that Plk4 localizes to distinct subcentrosomal regions in a temporally and spatially regulated manner, and that Cep192 and Cep152 serve as two distinct scaffolds that recruit Plk4 to centrosomes in a hierarchical order. Interestingly, Cep192 and Cep152 competitively interacted with the cryptic polo box of Plk4 through their homologous N-terminal sequences containing acidic-α-helix and N/Q-rich motifs. Consistent with these observations, the expression of either one of these N-terminal fragments was sufficient to delocalize Plk4 from centrosomes. Furthermore, loss of the Cep192- or Cep152-dependent interaction with Plk4 resulted in impaired centriole duplication that led to delayed cell proliferation. Thus, the spatiotemporal regulation of Plk4 localization by two hierarchical scaffolds, Cep192 and Cep152, is critical for centriole biogenesis.},
	Title = {Hierarchical recruitment of Plk4 and regulation of centriole biogenesis by two centrosomal scaffolds, Cep192 and Cep152.},
	Ty = {JOUR},
	Url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=24277814&retmode=ref&cmd=prlinks},
	Year = {2013},
	Bdsk-Url-1 = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=24277814&retmode=ref&cmd=prlinks},
	Bdsk-Url-2 = {http://dx.doi.org/10.1073/pnas.1319656110}}

