@article{Bilic2007,
	Address = {Division of Molecular Embryology, Deutsches Krebsforschungszentrum, Im Neuenheimer Feld 280, D-69120 Heidelberg, Germany.},
	Author = {Bilic, Josipa and Huang, Ya-Lin and Davidson, Gary and Zimmermann, Timo and Cruciat, Cristina-Maria and Bienz, Mariann and Niehrs, Christof},
	Booktitle = {Science},
	Date = {2007/00/15},
	Date-Added = {2014-07-24 13:27:28 +0000},
