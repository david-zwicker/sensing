@article{Nogales-Cadenas2009,
	Address = {Computer Architecture Department, Complutense University of Madrid, Madrid, Spain.},
	Author = {Nogales-Cadenas, Rub{\'e}n and Abascal, Federico and D{\'\i}ez-P{\'e}rez, Javier and Carazo, Jos{\'e}Mar{\'\i}a and Pascual-Montano, Alberto},
	Booktitle = {Nucleic acids research},
	Date = {2009/00/01},
