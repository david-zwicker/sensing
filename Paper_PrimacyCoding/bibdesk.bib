	Address = {Mathematical Biosciences Institute, 231 West 18th Avenue, Ohio State University, Columbus, OH 43210, USA.},
	Author = {Craciun, Gheorghe and Tang, Yangzhong and Feinberg, Martin},
	Booktitle = {Proceedings Of The National Academy Of Sciences Of The United States Of America},
	Date = {2006/00/06},
