	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.ceb.2009.01.008}}

@article{Cowan2006,
	Address = {Max Planck Institute, Molecular Cell Biology \& Genetics, Pfotenhauerstrasse 108, 01307 Dresden, Germany. cowan{\char64}mpi-cbg.de},
	Author = {Cowan, Carrie R and Hyman, Anthony A},
	Booktitle = {Nature Cell Biology},
	Date = {2006/00/01},
	Date-Added = {2014-07-24 13:27:28 +0000},
	Date-Modified = {2015-11-20 14:44:48 +0000},
	Doi = {10.1038/ncb1511},
	Id = {501},
	Journal = {Nat. Cell Biol.},
	Keywords = {Centrosome},
	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2006/Cowan/Nat.%20Cell%20Biol.%202006%20Cowan%20-%20Cyclin%20E-Cdk2%20temporally%20regulates%20centrosome%20assembly%20and%20establishment%20of%20polarity%20in%20Caenorhabditis%20elegans%20embryos.pdf},
	Month = {00},
	N2 = {Establishment of polarity in C. elegans embryos is dependent on the centrosome. The sperm contributes a pair of centrioles to the egg and these centrioles remain incapable of polarizing the cortex while the egg completes meiosis. Coincident with the establishment of polarity, the centrioles recruit centrosomal proteins, several of which are required for polarity, suggesting that the temporal regulation of centrosome assembly may control the initiation of polarization. We found that cyclin E-Cdk2 is required for the establishment of polarity. Cyclin E-Cdk2 controls the recruitment of centrosomal proteins specifically at the time of polarity establishment. Cyclin E is required for several examples of asymmetric cell division and fate determination in C. elegans and Drosophila. Here, we suggest a possible mechanism for cyclin E-Cdk2-dependent differentiation: the establishment of cortical polarity by the centrosome.},
	Number = {12},
	Pages = {1441--1447},
	Publisher = {Nature Publishing Group},
	Title = {Cyclin E-Cdk2 temporally regulates centrosome assembly and establishment of polarity in Caenorhabditis elegans embryos},
	Ty = {JOUR},
	Url = {http://www.nature.com/ncb/journal/v8/n12/full/ncb1511.html},
	Volume = {8},
	Year = {2006},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.nature.com/ncb/journal/v8/n12/full/ncb1511.html},
