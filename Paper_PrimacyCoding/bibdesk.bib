	Local-Url = {file://localhost/Users/zwicker/Documents/Papers2/Articles/2000/Valero/Biochemical%20Journal%202000%20Valero%20-%20Kinetics%20of%20a%20self-amplifying%20substrate%20cycle%20ADP-ATP%20cycling%20assay.pdf},
	Month = {00},
	N2 = {A kinetic study of an ATP-ADP amplification cyclic system involving the enzymes adenylate kinase, pyruvate kinase and L-lactate dehydrogenase has been made. The stoichiometry of the cycle is 2:1, because two molecules of ADP are synthesized from one each of ATP and AMP, and one molecule of ADP is converted back into one of ATP at each turn of the cycle. This results in a continuous exponential increase in the concentrations of ATP and ADP in the reaction medium, according to the equations obtained. This is therefore a substrate cycle that amplifies itself, the cycling rate increasing continuously with time. The background signal of the reagent was reduced by using apyrase to degrade ATP and ADP in the reagent, permitting detection limits as low as 16 pmol of ATP and/or ADP in a continuous spectrophotometric assay.},
	Number = {1},
	Pages = {237--243},
	Title = {Kinetics of a self-amplifying substrate cycle: ADP-ATP cycling assay.},
	Ty = {JOUR},
	Url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10926849&retmode=ref&cmd=prlinks},
	Volume = {350 Pt 1},
	Year = {2000},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10926849&retmode=ref&cmd=prlinks}}

